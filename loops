//for loop//
#include<stdio.h>
int main()
{
    int i,n;
    printf("\n Enter the limit:");
    scanf("%d",&n);
    for(i=1;i<=n;i+=2)
    {
        printf("\n%d",i);
    }
    return 0;
}


//while//
#include<stdio.h>
int main()
{
    int i=0,n;
    printf("\n enter the limit:");
    scanf("%d",&n);
    while(i<=n)
    {
        printf("\n%d",i);
        i++;
    }
    return 0;

}


//dowhile//
#include<stdio.h>
int main()
{
    int i=0,n;
    printf("\n enter the limit:");
    scanf("%d",&n);
    while(i<=n)
    {
        printf("\n%d",i);
        i++;
    }
    return 0;

}
